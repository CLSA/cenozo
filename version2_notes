Change 'service_name' in settings.local.ini.php to 'instance_name'
Change '/app/service.class.php' to '/app/application.class.php' in cron.hourly
Move setting files out of web tree (back one directory)
Copy the /var/www/html/webphone from Drummer to each webserver
For all databases, change the default character set to UTF8:
  ALTER DATABASE <db_name> CHARACTER SET utf8 COLLATE utf8_unicode_ci;
Create the recordings in F1:
  USE patrick_sabretooth_f1;

  INSERT IGNORE INTO recording( rank, name, record, timer ) VALUES
  ( 1, "REY Sound Test", false, NULL ),
  ( 2, "REY Word List", false, NULL ),
  ( 3, "REY I", true, 90 ),
  ( 4, "Animal List", true, 60 ),
  ( 5, "Counting to 20", true, 30 ),
  ( 6, "Alphabet", true, 30 ),
  ( 7, "MAT Alternation", true, 30 ),
  ( 8, "REY II", true, 60 );

  INSERT INTO recording_file( recording_id, language_id, filename )
  SELECT recording.id, language.id, "custom/english_sentence_mono_fixed.v1"
  FROM recording, patrick_cenozo.language
  WHERE recording.rank = 1
  AND language.code = "en";

  INSERT INTO recording_file( recording_id, language_id, filename )
  SELECT recording.id, language.id, "custom/french_sentence_mono_fixed.v1"
  FROM recording, patrick_cenozo.language
  WHERE recording.rank = 1
  AND language.code = "fr";

  INSERT INTO recording_file( recording_id, language_id, filename )
  SELECT recording.id, language.id, "custom/english_wordlist_mono_fixed.v1"
  FROM recording, patrick_cenozo.language
  WHERE recording.rank = 2
  AND language.code = "en";

  INSERT INTO recording_file( recording_id, language_id, filename )
  SELECT recording.id, language.id, "custom/french_wordlist_mono_fixed.v1"
  FROM recording, patrick_cenozo.language
  WHERE recording.rank = 2
  AND language.code = "fr";
